{
  "name": "SendIt",
  "version": "1.0.0",
  "engines": {
    "node": "8.x"
  },
  "description": "SendIt is a courier service that helps users deliver parcels to different destinations, SendIt provides courier quotes based on weight categories",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel Server -s -d dist",
    "test": "nyc mocha --require babel-core/register ./Server/test/**.test.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon bin/dev",
    "start": "node bin/production",
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development npm install --only=dev --dev",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;",
    "postinstall": "npm run build",
    "db_migrations": " babel-node Server/dataMigrations/createTables.js --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobbylemm/SendIt.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bobbylemm/SendIt/issues"
  },
  "homepage": "https://github.com/bobbylemm/SendIt#readme",
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "nodemailer": "^4.7.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "pg": "^7.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "prettier": "^1.15.2"
  }
}
